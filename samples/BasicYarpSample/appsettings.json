{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "minimumroute": {
        "ClusterId": "minimumcluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "minimumcluster": {
        "LoadBalancingPolicy": "Weight",
        "Destinations": {
          "minimumroute/example.com": {
            "Address": "http://www.example.com/",
            "Weight":50
          },
          "minimumroute/exampl1e.com": {
            "Address": "http://foo.bar.com/",
            "Weight":50
          }
        }
      }
    }
  }
}
