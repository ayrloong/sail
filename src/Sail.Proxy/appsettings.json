{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      }
    }
  },
  "Certificate": {
    "DefaultPath": "/etc/ssl/certs/tls.crt",
    "DefaultKeyPath": "/etc/ssl/certs/tls.key"
  },
  "ControllerUrl": "http://localhost:8000"
}
