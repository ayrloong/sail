{
  "ReverseProxy":{
    "Routes":{
      "route":{
        "ClusterId":"cluster",
        "Match":{
          "Path":"/lq"
        }
      },
      "route1": {
        "ClusterId":"cluster",
        "Match": {
          "Path": "/test/{**any}"
        },
        "Clusters": [
          {
            "ClusterId": "cluster",
            "Weight": 100
          }
        ]
      }
    },
    "Clusters":{
      "cluster":{
        "Destinations":{
          "route/example.com":{
            "Address":"https://example.com"
          }
        }
      },
      "cluster1":{
        "Destinations":{
          "route1/example.com":{
            "Address":"http://localhost:5010"
          }
        }
      }
    }
  }
}
