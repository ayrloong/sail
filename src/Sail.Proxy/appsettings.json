{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      },
      "Https": {
        "Url": "https://+:443"
      }
    }
  },
  "Certificate": {
    "DefaultPath": "/etc/ssl/certs/tls.crt",
    "DefaultKeyPath": "/etc/ssl/certs/tls.key"
  }
}
