{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      }
    }
  },
  "Certificate": {
    "DefaultPath": "/etc/ssl/certs/tls.crt",
    "DefaultKeyPath": "/etc/ssl/certs/tls.key"
  },
  "Receiver": {
    "ControllerUrl": "http://localhost:8000"
  },
  "Consul": {
    "Address": "http://127.0.0.1:8500"
  }
}
